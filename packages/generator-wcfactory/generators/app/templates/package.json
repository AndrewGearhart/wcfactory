{
  "name": "@rhelements/<%= elementName %>",
  "wcfactory": {
    "className": "<%= elementClassName %>",
    "elementName": "<%= elementName %>"
  },
  "version": "0.0.1",
  "description": "<%= readmeName %> element for <%= customElementClass %>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "<%= elementName %>.umd.js",
  "module": "<%= elementName %>.es5.js",
  "es2015": "<%= elementName %>.js",
  "amd": "<%= elementName %>.amd.js",
  "scripts": {
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "npm run build && open ./src && concurrently --kill-others \"npm run watch\" \"npm run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "cd build/es6 && polymer serve --npm --module-resolution=node --open",
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@rhelements/<%= elementName %>/test/",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "<%= author %>"
  },
  "license": "<%= license %>",
  "generator-rhelement-version": "<%= generatorRhelementVersion %>",
  "dependencies": {
    <% if (sassLibraryPkg) { %>"<%= sassLibraryPkg %>": "*",<% } %>
    <%- libraryPkg %>
    "@rhelements/rhelement": "*"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.1.1",
    "concurrently": "^4.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-connect": "^5.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify-es": "^1.0.4",
    "lighthouse": "^2.x",
    "lodash": "^4.17.10",
    "polymer-cli": "^1.7.1",
    "polymer-build": "^3.0.4",
    "uglify-es": "^3.3.9",
    "wct-browser-legacy": "^1.0.1"
  }
}
