{
  "name": "@wcfactory/cli",
  "description": "CLI for managing your custom elements at scale.",
  "version": "0.3.1",
  "author": "heyMP @heyMP",
  "bin": {
    "wcf": "./bin/run"
  },
  "bugs": "https://github.com/elmsln/wcfactory/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@wcfactory/common": "^0.3.0",
    "@wcfactory/generator-wcfactory": "^0.3.1",
    "glob": "^7.1.3",
    "inquirer": "^6.2.0",
    "inquirer-recursive": "^0.0.5",
    "listr": "^0.14.2",
    "listr-update-renderer": "^0.4.0",
    "listr-verbose-renderer": "^0.4.1",
    "lodash": "^4.17.11",
    "tslib": "^1",
    "yeoman-environment": "^2.3.3",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.19.0",
    "@oclif/test": "1.2.1",
    "@oclif/tslint": "3.1.0",
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "@types/node": "10.11.4",
    "chai": "4.2.0",
    "globby": "8.0.1",
    "mocha": "5.2.0",
    "nyc": "13.0.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/elmsln/wcfactory",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wcf",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "elmsln/wcfactory",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
